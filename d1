 CREATE USER 'emplyoee_user'@'localhost' 
 IDENTIFIED BY 'password';
GRANT ALL PRIVILEGES ON *.* 
TO 'emplyoee_user'@'localhost';
FLUSH privileges;

CREATE DATABASE EMP;
USE EMP;
CREATE TABLE employee (
EMPNO INT,
ENAME VARCHAR(50),
JOB VARCHAR(50),
MGR_NO INT,
SAL DECIMAL(10, 2),
COMMISSION DECIMAL(10, 2)
);
INSERT INTO Employee(EMPNO, ENAME, JOB, MGR_NO, SAL,COMMISSION) VALUES(1, 'ira', 'developer', 2, 58000,500);
INSERT INTO Employee(EMPNO, ENAME, JOB, MGR_NO, SAL,COMMISSION) VALUES(2, 'yuva', 'manager', 4, 68000,750);
INSERT INTO Employee (EMPNO, ENAME, JOB, MGR_NO, SAL,COMMISSION)VALUES (3, 'aarav', 'Salesperson', 6, 47000,400);
rollback;
ALTER TABLE employee
 ADD CONSTRAINT employee PRIMARY KEY(EMPNO);
 ALTER TABLE Employee
MODIFY EMPNO INT NOT NULL,
MODIFY ENAME VARCHAR(50) NOT NULL,
MODIFY SAL DECIMAL(10, 2) NOT NULL;
